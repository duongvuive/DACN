@model IEnumerable<DACN3.Models.Notification>

@{
    ViewData["Title"] = "ListNotification";
    Layout = "~/Views/Shared/LayoutInventory.cshtml";
}

<h1>ListNotification</h1>

<table class="table">
    <thead>
        <tr>
