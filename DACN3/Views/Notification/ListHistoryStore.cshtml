@model IEnumerable<DACN3.Models.ViewModel.ViewImportExportWarehouse>

@{
    ViewData["Title"] = "Lịch sử nhập xuất";
    Layout = "~/Views/Shared/LayoutInventory.cshtml";
}
<form action="/Notification/ListHistoryStore" method="get">
<div class="report">
    <div class="cardHeader">
        <h2>DANH SÁCH LỊCH SỬ KHO</h2>
    </div>
        <div class="filter">
            <label for="status">Trạng thái:</label>
    <select id="status" name="Status">
        <option value="">-- Tất cả --</option>
        <option value="Nhập Kho">Nhập Kho</option>
        <option value="Xuất Kho">Xuất Kho</option>
        <!-- Các trạng thái khác bạn muốn lọc -->
    </select>
            <button class="filter-button" style="background-color: #28a745;">Lọc trạng thái</button>
            </div>

<table class="table">
    <thead>
        <tr>
            <th>
                Tên thiết bị
            </th>
            <th>
                Kho
            </th>
            <th>
                Trụ sở
            </th>
            <th>
                Thời gian
            </th>
            <th>
                Trạng thái
            </th>
            <th>
               Quản lý kho
            </th>
            <th>
                Số lượng
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NameDevice)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NameWarehouse)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Area)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Status)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WarehouseMagement)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Amount)
            </td>
        </tr>
}
    </tbody>
</table>
</div>
</form>
    <script src="/js/script.js"></script>
    <script type="module" src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.esm.js"></script>
    <script nomodule src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.js"></script>
<style>
    tr td {
        text-align: center;
    }
</style>